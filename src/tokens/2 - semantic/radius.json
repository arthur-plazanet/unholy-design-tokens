{
  "radius": {
    "light": {
      "value": "calc(var(--border-radius-base) / var(--border-radius-scale))"
    },
    "medium": {
      "value": "calc(var(--border-radius-base) * var(--border-radius-scale))"
    },
    "heavy": {
      "value": "calc(var(--border-radius-base) * 2 * var(--border-radius-scale))"
    }
  }
}
