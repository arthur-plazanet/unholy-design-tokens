{
  "name": "style-dictionary-example-complete",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "build": "style-dictionary build --config src/style-dictionary.config.js",
    "build:verbose": "style-dictionary build --config src/style-dictionary.config.js --verbose",
    "clean": "rm -rf build",
    "tokens:typography": "node scripts/generate-typography-tokens.js",
    "tokens:build": "pnpm tokens:typography && node --loader ts-node/esm ./node_modules/.bin/style-dictionary",
    "tokens:build1": "node --loader ts-node/esm src/style-dictionary.config.ts build"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/tinycolor2": "1.4.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "style-dictionary": "^4.0.0-prerelease.30",
    "tinycolor2": "1.6.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.50.0"
  }
}
