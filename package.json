{
  "name": "style-dictionary-example-complete",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "type": "module",
  "scripts": {
    "clean": "rm -rf build",
    "tokens:typography": "node scripts/generate-typography-tokens.js",
    "build-tokens": "node scripts/generate-typography-tokens.js && node src/build-tokens.js ",
    "build-tokens:verbose": "node scripts/generate-typography-tokens.js && node src/build-tokens.js --verbose",
    "build": "build-tokens -- --buildPath=./build/ --source=./src/tokens/ --destination=./build/ --test",
    "build:verbose": "pnpm exec bin/build-tokens.js --verbose",
    "lint": "eslint .",
    "format": "prettier --write src/**/*.ts src/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.2",
    "@eslint/json": "^0.14.0",
    "@eslint/markdown": "^7.5.1",
    "@types/tinycolor2": "1.4.6",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "prettier": "3.7.4",
    "style-dictionary": "^4.0.0-prerelease.30",
    "tinycolor2": "1.6.0",
    "typescript-eslint": "^8.50.0"
  },
  "bin": {
    "build-tokens": "./bin/build-tokens.js"
  },
  "dependencies": {
    "commander": "^14.0.2"
  }
}
